{%- if customer.tags contains 'admin' -%}
<section class="header-bg-inner">
    <div class="page-width">
        <div class="text-center">
            {% include 'breadcrumb' %}
            <h1>{{ page.title }}</h1>

        </div>
    </div>
</section>
<div class="bottom-pages-section">
    <div class="page-width body">
        <div class="grid">
            <div class="grid__item medium-up--five-sixths medium-up--push-one-twelfth">
                <div class="rte list section">

                    <table>
                        <thead>
                            <th>Design</th>
                            <th>Date</th>
                        </thead>
                        <tbody id="tbody">
                        </tbody>
                    </table>
                </div>
            </div>
        </div>
    </div>
</div>
<style>
    div.body a {
        color: #616FC5;
        line-height: 2em;
    }
</style>
<script>
    document.addEventListener("DOMContentLoaded", function(event) {
        $.ajax({
            url: 'https://api.panacche.com/request/list',
            method: 'POST',
            async: true,
            jsonpCallback: 'dp',
            dataType: 'jsonp',
            contentType: 'application/json; charset=utf-8',
            success: function(response) {
                $('#tbody').html(response);
            },
            error: function(XMLHttpRequest) {
                console.log(XMLHttpRequest);
            }
        });
    });
</script>
{%- endif -%}