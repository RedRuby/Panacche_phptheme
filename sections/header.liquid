                                    <a href="/account/logout">
                                        <i class="fas fa-sign-out-alt mr-2"></i> Logout
                                    </a>

                                </p>
                            </div>
                        </li>
                        {% else %}

                        <li class="nav-item registerLink">
                            <a class="nav-link" href="/account">Register</a>
                        </li>
                        {% endif %} {% endif %}

                        <!---->
